{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "basic": {
      "format": "%(asctime)s %(name)s %(levelname)s %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "file_info": {
      "class": "logging.FileHandler",
      "level": "INFO",
      "formatter": "basic",
      "filename": "telesk_app_dev.log"
    },
    "file_error": {
      "class": "logging.FileHandler",
      "level": "ERROR",
      "formatter": "basic",
      "filename": "telesk_app_dev_error.log"
    }
  },
  "loggers": {
    "telesk": {
      "level": "INFO",
      "handlers": ["file_info", "file_error"],
      "propagate": true
    },
    "sqlalchemy.engine": {
      "level": "INFO",
      "handlers": ["file_info", "file_error"],
      "propagate": true
    },
    "model": {
      "level": "INFO",
      "handlers": ["file_info", "file_error"],
      "propagate": true
    }
  }
}
